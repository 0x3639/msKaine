# Telegram Bot
BOT_TOKEN=your_bot_token_here

# Database
POSTGRES_PASSWORD=kaine_password
DATABASE_URL=postgresql://kaine:${POSTGRES_PASSWORD}@localhost:5432/kaine_bot

# Redis (password is optional â€” leave empty to disable auth)
# docker-compose automatically builds REDIS_URL from REDIS_PASSWORD
# For local dev without Docker, set REDIS_URL manually:
REDIS_PASSWORD=
REDIS_URL=redis://localhost:6379

# Zenon Network
ZENON_NODE_HTTP=https://node.zenonhub.io:35997
ZENON_NODE_WS=wss://node.zenonhub.io:35998
ZENON_MNEMONIC=your_mnemonic_here

# Health Check (0 = disabled)
HEALTH_PORT=0

# Bot Settings
LOG_LEVEL=info
NODE_ENV=development
BOT_OWNER_ID=0
